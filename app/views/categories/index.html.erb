<h2 align="center">Listing All Categories</h2>

<div align="center">
  <%= will_paginate %>
  <% @categories.each do |category| %>
    <ul class="listing">
      <li class="article-title">
        <%= link_to "#{category.name}", category_path(category) %>
      </li>
    </ul>
  <% end %>
  <%= will_paginate %>
</div>
